###
### TOOLBAR
###
view.toolbar=new-file open-file save print send - reload - undo redo - cut \
	copy paste - find find-next - make compile prev-error next-error - \
	options exit - help@index.html
new-file.icon=filenew.gif
open-file.icon=fileopen.gif
save.icon=save.gif
print.icon=fileprint.gif
send.icon=send.gif
reload.icon=reload.gif
undo.icon=undo.gif
redo.icon=redo.gif
cut.icon=editcut.gif
copy.icon=editcopy.gif
paste.icon=editpaste.gif
find.icon=search.gif
find-next.icon=next.gif
make.icon=make.gif
compile.icon=compile.gif
prev-error.icon=back.gif
next-error.icon=forward.gif
options.icon=configure.gif
exit.icon=exit.gif
help@index.html.icon=contents.gif

