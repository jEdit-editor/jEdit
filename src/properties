### JEDIT PROPERTIES FILE

### Server
# Server is on
server=on
# Server is off
#server=off
# Default port file
server.portfile=.jedit-server

### Autosave
# Autosave is on
autosave=on
# Autosave is off
#autosave=off
# Autosave interval (in seconds)
autosave.interval=30

### Number of backups to make (0 = no backups)
backups=1

### Line separator
### If none is specified, the OS default will be used
# Unix
#line.separator=\n
# DOS/Windows
#line.separator=\r\n
# MacOS
#line.separator=\r

### Look and feel
# Cross platform
lf=xp
# System
#lf=system
# Motif
#lf=com.sun.java.swing.plaf.motif.MotifLookAndFeel
# Windows
#lf=com.sun.java.swing.plaf.windows.WindowsLookAndFeel

### Number of recent files to remember
maxrecent=8

### Name for untitled files
# {0} - the number of the file
untitled=Untitled {0}

### Editor window
# Normally, [xywh] is calculated to center the window on the screen.
# Override this only if you don't like this behaviour.
#editor.x=100
#editor.y=100
#editor.w=600
#editor.h=400

### Editor font
editor.font=Monospaced
editor.fontsize=12
# This can be 0 for plain text, 1 for bold, 2 for italic or 3 for bold italic
editor.fontstyle=0

### Tab width
editor.tabsize=8

### Line wrap policy
editor.linewrap=off
# Wrap at character
#editor.linewrap=char
# Wrap at word
#editor.linewrap=word

### Auto indent
editor.autoindent=on
#editor.autoindent=off

### About dialog
about.title=About jEdit
about.message=jEdit version {0} build {1}, Copyright (C) 1998 Slava Pestov.\n\
	jEdit comes with ABSOLUTELY NO WARRANTY. This is free software,\n\
	and you are welcome to redistribute it under certain conditions;\n\
	see the COPYING file for details.

### IO Error dialog
# {0} - the file name
# {1} - the error message
ioerror.title=I/O Error
ioerror.message=An I/O error occured in buffer {0} ({1})

### File not found dialog
# {0} - the file name
notfounderror.title=File Not Found
notfounderror.message=The file could not be found: {0}

### Open URL dialog
openurl.title=Open URL
openurl.message=URL to open:

### Open file dialog
openfile.title=Open File

### Save To URL dialog
saveurl.title=Save To URL
saveurl.message=URL to save file to:

### Save file dialog
savefile.title=Save File
### Go to line dialog
gotoline.title=Go To Line
gotoline.message=Line number to go to:

### Set marker dialog
setmarker.title=Set Marker
setmarker.message=Marker name:

### Clear marker dialog
clearmarker.title=Clear Marker
clearmarker.message=Marker to clear:

### Go to marker dialog
gotomarker.title=Go To Marker
gotomarker.message=Marker to jump to:

### File not saved dialog
notsaved.title=File Not Saved
# {0} - file name
notsaved.message=Save changes to {0}?

### Autosave file exists dialog
autosaveexists.title=Autosave File Exists
# {0} - autosave file name
autosaveexists.message=An autosave file {0} was found. This probably means\n\
	that jEdit crashed before you had a chance to save. If you do not\n\
	need the old unsaved information, you can delete this file.

### Status bar
# {0} - the file name
# {1} - 1 if the file is not on disk
# {2} - 1 if the file has changed since last save
# {3} - the number of lines in the buffer
# {4} - the current line number
# {5} - the percentage of the file displayed
status=({1,choice,0#--|1#**}:{2,choice,0#--|1#**}) {0} (Line {3}/{4} {5}%)

### Menubar
editor_mbar=file edit plugins buffers help
file=new_file new_view open open@/url %open_recent - close_file \
	close_view save save@/as save@/url - exit
file.label=File
new_file.label=New File
new_file.shortcut=C-n
new_view.label=New View
new_view.shortcut=CS-n
open.label=Open File...
open.shortcut=C-o
open@/url.label=Open URL...
open@/url.shortcut=C-u
open_recent.label=Open Recent
no_recent.label=No Recent Files
close_file.label=Close File
close_file.shortcut=C-w
close_view.label=Close View
close_view.shortcut=CS-w
save.label=Save
save.shortcut=C-s
save@/as.label=Save As...
save@/as.shortcut=CS-s
save@/url.label=Save To URL...
save@/url.shortcut=CS-u
exit.label=Exit
exit.shortcut=C-q
edit=undo redo - cut copy paste clear select_all - find find@/next \
	find@/selection replace replace@/all goto_line - set_marker \
	%clear_marker %goto_marker
edit.label=Edit
undo.label=Undo
undo.shortcut=C-z
redo.label=Redo
redo.shortcut=CS-z
cut.label=Cut
cut.shortcut=C-x
copy.label=Copy
copy.shortcut=C-c
paste.label=Paste
paste.shortcut=C-v
clear.label=Clear
clear.shortcut=CS-x
select_all.label=Select All
select_all.shortcut=C-a
find.label=Find...
find.shortcut=C-f
find@/next.label=Find Next
find@/next.shortcut=C-g
find@/selection.label=Find Selection
find@/selection.shortcut=C-h
replace.label=Replace
replace.shortcut=C-r
replace@/all.label=Replace All
replace@/all.shortcut=CS-r
goto_line.label=Go To Line...
goto_line.shortcut=C-l
set_marker.label=Set Marker...
set_marker.shortcut=C-m
clear_marker.label=Clear Marker
goto_marker.label=Go To Marker
no_markers.label=No Markers Defined
plugins.label=Plugins
no_plugins.label=No Plugins
buffers.label=Buffers
help=about - help@README help@INSTALL help@COPYING help@BUGS help@TODO - \
	help@commands.txt help@menus.txt help@plugins.txt help@props.txt \
	help@server.txt
help.label=Help
about.label=About jEdit...
help@README.label=Readme
help@INSTALL.label=Installation
help@COPYING.label=Copying
help@BUGS.label=Known Bugs
help@TODO.label=To Do
help@commands.txt=Commands
help@menus.txt.label=Menus
help@plugins.txt.label=Plugins
help@props.txt.label=Properties
help@server.txt.label=Server
