### JEDIT PROPERTIES FILE

### Server
# Server is on
server=on
# Server is off
#server=off
# Default port file
server.portfile=.jedit-server

### Autosave
# Autosave interval (in seconds)
autosave=30

### Number of backups to make (0 = no backups)
backups=1

### Line separator
### If none is specified, the OS default will be used
# Unix
#line.separator=\n
# DOS/Windows
#line.separator=\r\n
# MacOS
#line.separator=\r

### Look and feel
# Cross platform
lf=com.sun.java.swing.plaf.metal.MetalLookAndFeel
# Motif
#lf=com.sun.java.swing.plaf.motif.MotifLookAndFeel
# Windows
#lf=com.sun.java.swing.plaf.windows.WindowsLookAndFeel

### Number of recent files to remember
maxrecent=8

### Name for untitled files
# {0} - the number of the file
untitled=Untitled-{0}

### Editor window
# Title
# {0} - the current file name
editor.title=jEdit - {0}

# The width and height of the text area, in characters
editor.w=80
editor.h=30
# The x and y co-ordinate of the editor window
editor.x=100
editor.y=50

### Editor font
editor.font=Monospaced
editor.fontsize=12
# This can be 0 for plain text, 1 for bold, 2 for italic or 3 for bold italic
editor.fontstyle=0

### Tab width
editor.tabsize=8

### Line wrap policy
editor.linewrap=off
# Wrap at character
#editor.linewrap=char
# Wrap at word
#editor.linewrap=word

### Auto indent
editor.autoindent=on
#editor.autoindent=off

### Printing
margin.top=0.5
margin.left=0.5
margin.bottom=0.5
margin.right=0.5

### About dialog
about.title=About jEdit
about.message=jEdit version {0} build {1}, Copyright (C) 1998 Slava Pestov.\n\
	jEdit comes with ABSOLUTELY NO WARRANTY. This is free software,\n\
	and you are welcome to redistribute it under certain conditions;\n\
	see Help->Copying for details.

### IO Error dialog
# {0} - the error message
ioerror.title=I/O Error
ioerror.message=An I/O error occured ({0})

### File not found dialog
# {0} - the file name
notfounderror.title=File Not Found
notfounderror.message=The file could not be found: {0}

### Unknown error dialog
# {0} - the error
error.title=Unknown Error
error.message=An error occured: {0}

### Open URL dialog
openurl.title=Open URL
openurl.message=URL to open:

### Open file dialog
openfile.title=Open File

### Save To URL dialog
saveurl.title=Save To URL
saveurl.message=URL to save file to:

### Save file dialog
savefile.title=Save File
### Go to line dialog
gotoline.title=Go To Line
gotoline.message=Line number to go to:

### Set marker dialog
setmarker.title=Set Marker
setmarker.message=Marker name:

### Clear marker dialog
clearmarker.title=Clear Marker
clearmarker.message=Marker to clear:

### Go to marker dialog
gotomarker.title=Go To Marker
gotomarker.message=Marker to jump to:

### File not saved dialog
notsaved.title=File Not Saved
# {0} - file name
notsaved.message=Save changes to {0}?

### Autosave file exists dialog
autosaveexists.title=Autosave File Exists
# {0} - autosave file name
autosaveexists.message=An autosave file {0} was found.\n\
	This probably means that jEdit crashed before you could save.\n\
	Open it to see your unsaved data.

### Search and replace dialog
search.title=Search And Replace
search.find=Find:\ 
search.replace=Replace With:\ 
search.ignoreCase=Ignore Case
search.next=Find Next
search.replaceBtn=Replace
search.replaceAll=Replace All
search.close=Close

### Keep searching dialog
keepsearching.title=No More Matches Found
keepsearching.message=No more matches were found. Continue search from\n\
	beginning of buffer?

### Hypersearch dialog
hypersearch.title=HyperSearch
hypersearch.find=Search For:\ 
hypersearch.findBtn=Find All
hypersearch.close=Close
hypersearch.ignoreCase=Ignore Case

### Options dialog
options.title=Options
options.lf=Look And Feel 
options.lf.metal=Metal
options.lf.motif=Motif
options.lf.windows=Windows
options.opening=Opening Files
options.opening.server=Enable jEdit server (requred for jOpen)
options.opening.maxrecent=Maximum number of recent files in menu:\ 
options.saving=Saving Files
options.saving.autosave=Autosave frequency (secs, 0 = never):\ 
options.saving.backups=Maximum number of backups (0 = none):\ 
options.saving.newline=Line separator:\ 
options.saving.newline.unix=Unix
options.saving.newline.windows=Windows
options.saving.newline.mac=MacOS
options.editing=Editing Files
options.editing.autoindent=Auto Indent
options.editing.wrap=Wrap:\ 
options.editing.wrap.off=Off
options.editing.wrap.char=Character
options.editing.wrap.word=Word
options.editing.tabsize=Tab Size:\ 
options.editing.font=Font:\ 
options.printing=Print Margins (Inches)
options.printing.left=Left:\ 
options.printing.right=Right:\ 
options.printing.top=Top:\ 
options.printing.bottom=Bottom:\ 
options.ok=OK
options.cancel=Cancel

### Status bar
# {0} - the file name
# {1} - 1 if the file is not on disk
# {2} - 1 if the file is read only
# {3} - 1 if the file has changed since last save
# {4} - the number of lines in the buffer
# {5} - the current line number
# {6} - the percentage of the file displayed
status=({1,choice,0#--|1#**}{2,choice,0#:|1#/}{3,choice,0#--|1#**}) {0} \
	(Line {4}/{5} {6}%)

### Menubar
editor_mbar=file edit search plugins buffers help
file=new_file new_view open_file open_url open_selection %open_recent - \
	close_file close_view - save save_as save_url - print - exit
file.label=File
new_file.label=New File
new_file.shortcut=C-n
new_view.label=New View
new_view.shortcut=CS-n
open_file.label=Open File...
open_file.shortcut=C-o
open_url.label=Open URL...
open_url.shortcut=C-u
open_selection.label=Open Selection
open_selection.shortcut=CS-o
open_recent.label=Open Recent
no_recent.label=No Recent Files
close_file.label=Close File
close_file.shortcut=C-w
close_view.label=Close View
close_view.shortcut=CS-w
save.label=Save
save.shortcut=C-s
save_as.label=Save As...
save_as.shortcut=CS-s
save_url.label=Save To URL...
save_url.shortcut=CS-u
print.label=Print...
print.shortcut=C-p
exit.label=Exit
exit.shortcut=C-q
edit=undo redo - cut copy paste clear select_all - goto_line - \
	set_marker %clear_marker %goto_marker - options
edit.label=Edit
undo.label=Undo
undo.shortcut=C-z
redo.label=Redo
redo.shortcut=CS-z
cut.label=Cut
cut.shortcut=C-x
copy.label=Copy
copy.shortcut=C-c
paste.label=Paste
paste.shortcut=C-v
clear.label=Clear
select_all.label=Select All
select_all.shortcut=C-a
goto_line.label=Go To Line...
goto_line.shortcut=C-l
set_marker.label=Set Marker...
set_marker.shortcut=C-m
clear_marker.label=Clear Marker
goto_marker.label=Go To Marker
no_markers.label=No Markers Defined
options.label=Options...
search.label=Search
search=find find_next find_selection - replace replace_next replace_all - \
	hypersearch
find.label=Find...
find.shortcut=C-f
find_next.label=Find Next
find_next.shortcut=C-g
find_selection.label=Find Selection
find_selection.shortcut=C-h
replace.label=Replace
replace.shortcut=C-r
replace_next.label=Replace & Find Next
replace_next.shortcut=CS-g
replace_all.label=Replace All
replace_all.shortcut=CS-r
hypersearch.label=HyperSearch...
hypersearch.shortcut=CS-h
plugins.label=Plugins
no_plugins.label=No Plugins
buffers.label=Buffers
help=about help - help@README help@COPYING help@VERSION - help@starting.txt \
	help@menus.txt help@shortcuts.txt help@props.txt help@plugins.txt
help.label=Help
about.label=About jEdit...
help@README.label=Readme
help@COPYING.label=Copying
help@VERSION.label=Version Info
help@starting.txt.label=Getting Started
help@menus.txt.label=Menus
help@shortcuts.txt.label=Shortcuts
help@props.txt.label=Properties
help@plugins.txt.label=Plugins
